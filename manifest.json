{
  "manifest_version": 2,
  "name": "UwaterlooOpenRecSchedule",
  "version": "1.0",

  "description": "Makes the Uwaterloo open rec schedule more human readable",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.uwaterloo.ca/Facility/GetSchedule*"],
      "js": ["fixSchedule.js"]
    }
  ],
  "web_accessible_resources": [
	"scripts.js"
  ]

}